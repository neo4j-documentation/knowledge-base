<!-- Colors taken from the Neo4j style guide -->
<style>
  /* hack until row/column roles are supported in Asciidoctor: */
  /* https://github.com/asciidoctor/asciidoctor/issues/1338 */
  table.neo4j-supported-versions > tbody > tr:nth-child(1) > td > p,
  table.neo4j-supported-versions > tbody > tr:nth-child(2)> td > p,
  table.neo4j-supported-versions > tbody > tr:nth-child(3)> td > p {
    color: #60B389;
    font-weight: bold;
  }

  table.neo4j-supported-versions > tbody > tr:nth-child(n+4) > td > p {
    color: #DE6760;
    font-weight: normal;
  }

  .blue {
    color: #68BBF4
  }

  .blue-dark {
    color: #5CA6D9
  }

  .purple {
    color: #DC99F7
  }

  .purple-dark {
    color: #BD83D4
  }

  .pink {
    color: #F993AD
  }

  .pink-dark {
    color: #DE8299
  }

  .red {
    color: #FD756E
  }

  .red-dark {
    color: #DE6760
  }

  .orange {
    color: #FDD66E
  }

  .orange-dark {
    color: #EBB83A
  }

  .green {
    color: #6DCC9C
  }

  .green-dark {
    color: #60B389
  }

  .gray-darker {
    color: #30333A
  }

  .gray-dark {
    color: #525865
  }

  .gray {
    color: #9599A1
  }

  .gray-light {
    color: #D2D5DA
  }

  .gray-lighter {
    color: #EEF1F8
  }

  .gray-lightest {
    color: #F9FBFD
  }
  .big {
    font-size: larger
  }

  .small {
    font-size: smaller
  }

  .underline {
    text-decoration: underline
  }

  .overline {
    text-decoration: overline
  }

  .line-through {
    text-decoration: line-through
  }
</style>

<%= content %>

